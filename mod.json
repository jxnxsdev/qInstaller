  "dependencies": [],
  "lateModFiles": [],
  "libraryFiles": [],
  "fileCopies": [],
  "copyExtensions": [
    {
        "extension": "qsaber",
        "destination": "/sdcard/ModData/com.beatgames.beatsaber/Mods/Qosmetics/Whackers/"
    },
    {
        "extension": "qwall",
        "destination": "/sdcard/ModData/com.beatgames.beatsaber/Mods/Qosmetics/Boxes/"
    },
    {
        "extension": "qbloq",
        "destination": "/sdcard/ModData/com.beatgames.beatsaber/Mods/Qosmetics/Cyoobs/"
